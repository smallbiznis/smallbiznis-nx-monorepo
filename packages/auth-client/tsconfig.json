{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "types": [],
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "esnext",
    "target": "es2022",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
